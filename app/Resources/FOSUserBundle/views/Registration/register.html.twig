{% extends "FOSUserBundle::layout.html.twig" %}

{% block head_style %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/datepicker3.css') }}">
    <link rel="stylesheet" href="{{ asset('css/dropzone.css') }}">
{% endblock head_style %}

{% block head_script %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('js/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/bootstrap-datepicker.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/bootstrap-datepicker.fr.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/moment/moment.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/moment/locales.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/bootstrap-filestyle/bootstrap-filestyle.js') }}"></script>
{% endblock head_script %}

{% block fos_user_content %}
    {% include "FOSUserBundle:Registration:register_content.html.twig" %}
{% endblock fos_user_content %}

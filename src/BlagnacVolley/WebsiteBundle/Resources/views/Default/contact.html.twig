{% extends 'BlagnacVolleyWebsiteBundle::layout.html.twig' %}

{% block title %}Contact{% endblock%}

{% block content_row %}
    {% for label, flashes in app.session.flashbag.all %}
        {% for flash in flashes %}
            <div class="alert alert-{{ label }}">
                {{ flash }}
            </div>
        {% endfor %}
    {% endfor %}

    {{ form(form) }}
{% endblock %}

